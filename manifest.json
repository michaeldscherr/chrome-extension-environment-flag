{
    "name": "Environment Flag Example",
    "version": "1.0",
    "description": "Environment Flag Example Extension",
    "manifest_version": 2,
    "permissions": ["storage", "activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "options_page": "options/options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content/content.js"]
        }
    ]
}
